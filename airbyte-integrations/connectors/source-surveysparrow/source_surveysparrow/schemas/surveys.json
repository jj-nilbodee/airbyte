{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["integer"]
    },
    "name": {
      "type": ["string"]
    },
    "archived": {
      "type": ["boolean"]
    },
    "survey_type": {
      "type": ["string"],
      "enum": ["Conversational", "ClassicForm", "OfflineApp", "NPS", "NPSChat", "CES", "CESChat", "CSAT", "CSATChat", "Employee360"]
    },
    "created_at": {
      "type": ["string"],
      "format": "timestamp"
    }
  }
}
